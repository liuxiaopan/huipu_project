
.container-fluid .page-header{
    border-bottom: 1px solid #0096ff;
}
.nova-content,.nova-content-tab{
    padding-top: 15px;
}
.nova-show{
    padding-left: 5px;
}
.nova-label{
    display: inline-block;
    margin-bottom: 0;
    font-weight: bold;
}
.nova-change{
    font-size: 12px;
}
.nova-content .nav-tabs{
    border-bottom: 1px solid #428bca;
}

.nova-content .nav-tabs>li.active>a{
    border: 1px solid #428bca;
    border-bottom-color: transparent;
}
.nova-content .nav>li>a:hover{
    background-color: #428bca;
    color: #ffffff;
}
.nova-btn-group {
    margin-right: 2%;
}
.form-group .form-control{
    font-size: 12px;
}
.form-group .form-control[disabled]{
    cursor: default;
}
.col-md-six{
    width: 50%;
    float: left;
}

.nova-content-tab .table-bordered>tbody>tr>td, .table-bordered>tbody>tr>th, .table-bordered>tfoot>tr>td, .table-bordered>tfoot>tr>th, .table-bordered>thead>tr>td, .table-bordered>thead>tr>th{
    font-size: 12px;
}
.nova-plan{
    padding-left: 15px;
}
.nova-footer{
    margin-bottom: 20px;
}
.col-sm-five{
    float: left;
    width: 40%;
    margin-right: 1%;
    margin-left: 3%;
}
.active{display:block;}


.nova-level{
    width: 40%;
    margin-right: 2%;
}
.nova-select {
    display: inline-block;
    width: 54%;
    /*margin-top: 10px;*/
}
.nova-left-menu ul{
    padding-left: 0px;
}
ul li{
    list-style: none;
}

.nova-glyphicon-ctrl, .row-ctrl{display: inline-block}
.nova-sec-case{
    font-size: 20px;
    font-weight: 600;
}
.container-fluid .page-header{
    margin-top: 20px;
}
.active{display:block;}
.nova-content .nav-tabs>li.active>a{
    border: 1px solid #428bca;
    border-bottom-color: transparent;
}
.nova-box .form-group .control-label{
    display: inline-block;
    padding: 0 6px;
}
.nova-content, .nova-content-tab{
    padding-top: 15px;
    width: 100%;
    padding-left: 20px;
}
#myTabContent span{
    font-weight: 600;
}
.nova-component-list{
    padding: 0;
}
.nova-case-ctrl{
    width: 40%;
    float: right;
}
.nova-case-ctrl button{
    margin-right: 15px;
}
.nova-component-list li{
    padding-left: 0;
}
#myTabContent form{
    overflow: hidden;
}
.nova-table-responsive{
    width: 90%;
    overflow-x: hidden;
}

.nova-card{
    font-size: 16px;
    font-weight: 600;
    color: blue;
    height: 30px;
    line-height: 30px;
    margin-bottom: 19px;
}
.nova-card-box{
    height: auto;
    border: 1px solid #428bca;
    padding: 10px;
    border-radius: 4px;
    margin-bottom: 10px;
}

.nova-task-desktop .nova-left-menu {
    margin-top: 71px;
}

.nova-task-desktop .nova-left-menu ul li {
    margin-bottom: 15px;
}