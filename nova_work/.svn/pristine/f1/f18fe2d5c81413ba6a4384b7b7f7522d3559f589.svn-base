.container-fluid .page-header{
    border-bottom: 1px solid #0096ff;
}
.nova-content,.nova-content-tab{
    padding-top: 15px;
}
.nova-show{
    padding-left: 5px;
}
.nova-label{
    display: inline-block;
    margin-bottom: 0;
    font-weight: bold;
}
.nova-change{
    font-size: 12px;
}
.nova-content .nav-tabs{
    border-bottom: 1px solid #428bca;
}

.nova-content .nav-tabs>li.active>a{
    border: 1px solid #428bca;
    border-bottom-color: transparent;
}
.nova-content .nav>li>a:hover{
    background-color: #428bca;
    color: #ffffff;
}
.nova-btn-group {
    margin-right: 2%;
}
.form-group .form-control{
    font-size: 12px;
}
.form-group .form-control[disabled]{
    cursor: default;
}
.col-md-six{
    width: 50%;
    float: left;
}

.nova-content-tab .table-bordered>tbody>tr>td, .table-bordered>tbody>tr>th, .table-bordered>tfoot>tr>td, .table-bordered>tfoot>tr>th, .table-bordered>thead>tr>td, .table-bordered>thead>tr>th{
font-size: 12px;
}
.nova-plan{
    padding-left: 15px;
}
.nova-footer{
    margin-bottom: 20px;
}
.col-sm-five{
    float: left;
    width: 40%;
    margin-right: 1%;
    margin-left: 3%;
}
.active{display:block;}

.nova-detail-plan-btn-group {
    text-align: right;
    padding-top: 25px;
    border-top: 1px solid #428bca;
    padding-bottom: 15px;
}
.nova-detail-plan-btn-group>button {
    margin-left: 5px;
}
.nova-detail-plan-right-panel {
    padding-bottom: 20px;
}
.nova-detail-plan-right-panel .form-group{
    margin-bottom: 5px;
}
.nova-tree-view-container {
    margin-left: 15px;
    margin-right: 15px;
}
.nova-tree-view-container .badge {
    background: none;
    color: #777;
 }
.nova-tree-view-planName, .nova-tree-view-sectionName {
    display: inline-block;
    margin-bottom: 10px;
}
.nova-tree-view-section ,.nova-tree-view-case {
    margin-right: -16px;
}

.nova-category-modal .treeview .list-group {
    height: 370px;
    overflow: scroll;
}
